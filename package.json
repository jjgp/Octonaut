{
  "name": "LostKitty",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "relay": "relay-compiler --src ./src --schema ./schema/github.graphql --client-schema ./schema/client.graphql --extensions js jsx",
    "relay-devtools": "relay-devtools",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "buffer": "^5.2.1",
    "moment": "^2.22.2",
    "react": "16.8.3",
    "react-native": "0.59.0",
    "react-native-device-info": "^0.24.3",
    "react-native-fast-image": "^5.1.0",
    "react-native-keychain": "^3.0.0",
    "react-navigation": "^3.0.0",
    "react-relay": "^1.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.4.0",
    "babel-plugin-relay": "^1.6.2",
    "jest": "^24.4.0",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "graphql": "^14.0.2",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "metro-react-native-babel-preset": "^0.53.0",
    "prettier": "^1.14.3",
    "react-test-renderer": "16.8.3",
    "relay-compiler": "^1.6.2",
    "relay-devtools": "^1.4.0"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ]
  },
  "lint-staged": {
    "./**/*.json": [
      "prettier --write",
      "git add"
    ],
    "./**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
